*{
  padding:0;
  margin:0;
}
img{
  border:none;
  outline:none;
}
ul{
  list-style:none;
}
body, html{
  width: 100%; height: 100%; position: relative; margin: 0; padding: 0;
}
body{
  font-size:16px;
  color:#333333;
  width:100%;
  height:100%;
  font-family: 'Muli', sans-serif;
}
.clear{
  float:none;
  clear:both;
}
a{
  text-decoration:none;
}
a:hover{
  text-decoration:underline;
}
.wrap{
  margin:0 auto;
  max-width:1075px;
}
.header{
  max-width:90%;
  margin:0 auto;
  padding:20px 0;
}

.top{
  border-bottom:3px solid #f73011;

}
.logo{
  float:left;
  width:40%;
}
.logo a,.logo span{
  display:inline-block;
  vertical-align:middle;

}
.topmenu{
  float:right;
  width:60%;
  text-align:right;
  padding-top:10px;
}
.logo p{
  font-weight:400;
  color:#333333;
  font-size:11px;
  text-align:center;
}
.logo a{text-decoration:none!important;text-align:center}
.logo a img{
  display:inline-block;
}
.logo span{
  color:#333333;
  font-size:16px;
  font-weight:400;
  padding-left:30px;
}
.topmenu a img{
  vertical-align:middle;
  padding-right:10px;
}
.topmenu a{
  font-weight:400;
  font-size:17px;
  display:inline-block;
  vertical-align:middle;
  padding:0 10px;
  color:#333;
  position:relative;
  min-height:38px;
  text-decoration:none!important;
}
.topmenu a:before {
  content: "";
  display: inline-block;
  height: 4px;
  background-color: #f13624;
  bottom: 0;
  position: absolute;
  width: 0;
  left: 50%;
  margin-left: 0;
  transition: all 0.3s

}
.topmenu a:hover:before {
  width: 50px;
  margin-left: -25px
}
.topmenu a.ph{
  font-weight:700;
}

.slide1{
  width:100%;
}
.bannerleft{
  float:left;
  width:60%;
  max-width:550px;
  padding-top:150px;
}
.footlogo p{
  font-weight:400;
  color:#333333;
  font-size:11px;
}
.footlogo{
  text-decoration:none!important;
}
.bannerright{
  float:right;
  width:40%;
  text-align:right;
  padding-top:70px;
}
.bx-wrapper .bannerright img{
  display:inline-block;
  width:auto;
  max-width:inherit;
}
.bannerleft h1{
  font-weight:800;
  color:#fff;
  font-size:35px;
  text-transform:uppercase;
  padding-bottom:15px;
}

.slidercon{
  position:absolute;
  width:100%;
  left:0;
  top:0;

}
.banner{
  position:relative
}
.bannercon{
  margin:0 auto;
  max-width:1075px;
}

.bannerleft p{
  font-weight:400;
  color:#fff;
  font-size:20px;
  line-height:27px;
  padding-bottom:20px;
}

.bannerleft a{
  display:inline-block;
}
.apps{
  margin-right:50px;

}

.mid p{
  font-family: 'Lato', sans-serif;
  font-weight:300;
  color:#767676;
  font-size:20px;
  line-height:28px;
}

.mid{
  padding:35px 0;
}
.gallery{
  background:#333333;
  padding:50px 0;
}

.gallery h2{
  color:#fff;
  font-weight:700;
  text-align:center;
  font-size:30px;
  background:url(<%= asset_path 'line.png' %>) no-repeat center bottom;
  padding-bottom:35px;
}
.gallery ul{
  padding-top:65px;
}
.gallery ul li:nth-child(even) {margin-left:8%;}
.colbox__figure {
  background:#2e2d2d;
  cursor:pointer;
  margin:0;
  max-height:280px;
  min-height:250px;
  overflow:hidden;
  position:relative;
  text-align:center;
  transition:background-color .35s;
  width:45%;
  display:inline-block;
  margin-bottom:25px;
}
.colbox__figure:hover {
  background:#821218
}
.colbox__figure:hover .colbox__caption::after,.colbox__figure:hover .colbox__caption::before,.colbox__figure:hover .colbox__description,.colbox__figure:hover .colbox__title {
  opacity:1;
  -webkit-transform:scale(1);
  transform:scale(1)
}
.colbox__figure:hover .colbox__img {
  opacity:.4;
  -webkit-transform:scale(1.05);
  transform:scale(1.05)
}
.colbox__img {
  display:block;
  height:250px;
  opacity:1;
  position:relative;
  transition:all .35s ease-out;
  width:100%
}
.colbox__caption {
  -webkit-backface-visibility:hidden;
  backface-visibility:hidden;
  box-sizing:border-box;
  color:#fff;
  font-size:1.25em;
  height:100%;
  left:0;
  padding:2em;
  position:absolute;
  top:0;
  width:100%
}
.colbox__caption:after,.colbox__caption:before {
  bottom:30px;
  content:'';
  left:30px;
  opacity:0;
  pointer-events:none;
  position:absolute;
  right:30px;
  top:30px;
  transition:opacity .35s,-webkit-transform .35s;
  transition:opacity .35s,transform .35s;
  transition:opacity .35s,transform .35s,-webkit-transform .35s
}
.colbox__caption{
  padding:10px;

}
.colbox__caption:before {
  border-bottom:1px solid #fff;
  border-top:1px solid #fff;
  -webkit-transform:scale(0,1);
  transform:scale(0,1);

}
.colbox__caption:after {
  border-left:1px solid #fff;
  border-right:1px solid #fff;
  -webkit-transform:scale(1,0);
  transform:scale(1,0);

}
.colbox__caption div {
  line-height:normal;
  position:relative;
  top:50%;
  -webkit-transform:translateY(-50%);
  transform:translateY(-50%)
}
.colbox__title {
  color:#fff;
  font-size:22px;
  letter-spacing:1px;
  margin:0;
  padding-top:0;
  text-shadow:0 1px 1px rgba(70,68,68,.8);
  -webkit-transform:translate3d(0,20px,0);
  transform:translate3d(0,20px,0);
  transition:-webkit-transform .35s;
  transition:transform .35s;
  transition:transform .35s, -webkit-transform .35s
}
.colbox__subheading {
  font-size:22px
}
.mdl-typography--text-uppercase{
  font-size:30px;
}
.colbox__description {
  color:#fff;
  font-size:16px;
  line-height:24px;
  margin:0;
  opacity:0;
  padding:10px 2em 0;
  text-shadow:0 1px 1px rgba(70,68,68,.8);
  text-transform:none;
  -webkit-transform:translate3d(0,20px,0);
  transform:translate3d(0,20px,0);
  transition:opacity .35s,-webkit-transform .35s;
  transition:opacity .35s,transform .35s;
  transition:opacity .35s,transform .35s,-webkit-transform .35s
}
.colbox__link {
  font-size:0;
  height:100%;
  left:0;
  opacity:0;
  position:absolute;
  text-indent:200%;
  top:0;
  white-space:nowrap;
  width:100%;
  z-index:1000
}
.u-isBlock {
  display: block;
}

.footer{
  background:#fff;
}
.footmenu{
