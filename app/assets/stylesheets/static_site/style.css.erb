}

.fixed{
  width:100%;
  position:fixed;
  top:0;
  left:0;
  z-index:99999;
  background:#fff;
}



@media only screen and (max-width:1075px) {
  .logo {
    float: none;
    width: auto;
    text-align: center;
  }
  .topmenu {
    float: none;
    width: auto;
    text-align: center;
    padding-top: 10px;
  }
  .header {
    max-width:inherit;
    margin: 0 auto;
    padding: 20px 15px;
  }
  .bannercon,.wrap{
    padding:0 15px;
  }
  .bannerright {
    padding-top: 15px;
  }
  .bx-wrapper .bannerright img {
    width: 50%;
    max-width: inherit;
  }
  .bannerleft {
    padding-top: 5%;
  }
  .foot3{
    text-align:center;
  }
  .foot3 span,.copy span ,.footlogo, .social, .copy span{
    display: block;
  }
  .foot3 span.sp{
    display:none;
  }
  .copy{
    text-align:center;
  }
  .footlogo{
    padding:15px 0;
  }
  .footapp {
    width: auto;
    float: none;
    text-align: center;
  }
  .footlinks {
    float: none;
    width: auto;
    padding-top: 30px;
    text-align: center;
  }
}
@media only screen and (max-width:800px) {
  .bannerleft h1{
    font-size:30px;
  }
  .apps {
    margin-right: 1%;
  }
  .bannerleft p {
    font-size: 17px;
    line-height: 22px;
  }
  .bannerleft img{
    width:100%;
  }
  .bannerleft a{
    width:45%;
  }
}
@media only screen and (max-width:700px) {
  .bannerleft h1 {
    font-size: 20px;
  }
  .mdl-typography--text-uppercase {
    font-size: 20px;
  }
  .colbox__subheading {
    font-size: 16px;
  }
}
@media only screen and (max-width:480px) {
  .bannerleft h1 {
    font-size: 13px;
  }
  .bannerleft p{
    display:none;
  }
  .gallery h2{
    font-size:22px;
  }
  .colbox__figure{
    width:100%;
    margin-left:0!important;
  }
  .apps3 {
    margin-right: 5px;
  }
  .apps2 a{
    width:48%;
  }
  .apps2 a img{
    width:100%;
  }
  .list1, .list2,.lst {
    border-right:0;
    margin-right:0;
    display:block;
    width:auto;
  }
  .list2 {
    padding:25px 0;
  }
  .social a{
    margin:0;
  }
  .fixed .topmenu{
    display:none;
  }
}
@media only screen and (max-width:360px) {
  .topmenu a{
    display:block;
    padding:5px 0;
  }
}
